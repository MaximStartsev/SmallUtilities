<UserControl x:Class="MaximStartsev.SmallUtilities.SearchJobCRM.Views.DetailedInfoView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MaximStartsev.SmallUtilities.SearchJobCRM.Views"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300" Width="500" Height="400">

        <Popup>
            <Grid Background="AliceBlue">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <TextBlock Text="Title"></TextBlock>
            </Grid>
        </Popup>

</UserControl>
